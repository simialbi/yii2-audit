{
	"name": "simialbi/yii2-audit",
	"description": "Audit",
	"keywords": ["yii2", "elfinder"],
	"type": "yii2-extension",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/simialbi/yii2-audit/issues",
		"source": "https://github.com/simialbi/yii2-audit"
	},
	"authors": [
		{
			"name": "Simon Karlen",
			"email": "simi.albi@gmail.com"
		}
	],
	"prefer-stable": true,
	"require": {
		"yiisoft/yii2": "~2.0",
		"yiisoft/yii2-bootstrap": "~2.0",
		"kartik-v/yii2-grid": "~3.1",
		"kartik-v/yii2-widget-datetimepicker": "~1.4"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"config": {
		"process-timeout": 1800,
		"secure-http": false,
		"fxp-asset": {
			"enable": false
		}
	},
	"autoload": {
		"psr-4": { "simialbi\\yii2\\audit\\": "" }
	}
}