{
	"name": "simialbi/yii2-audit",
	"description": "Audit",
	"keywords": [
		"yii2",
		"elfinder"
	],
	"type": "yii2-extension",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/simialbi/yii2-audit/issues",
		"source": "https://github.com/simialbi/yii2-audit"
	},
	"authors": [
		{
			"name": "Simon Karlen",
			"email": "simi.albi@gmail.com"
		}
	],
	"prefer-stable": true,
	"require": {
		"php": ">=5.6",
		"yiisoft/yii2": "^2.0.22",
		"kartik-v/yii2-grid": "^3.3.2",
		"kartik-v/yii2-widget-datetimepicker": "^1.4.9",
		"simialbi/yii2-simialbi-base": "^0.4.0"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"config": {
		"process-timeout": 1800,
		"secure-http": false,
		"fxp-asset": {
			"enable": false
		}
	},
	"autoload": {
		"psr-4": {
			"simialbi\\yii2\\audit\\": "src"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0.x-dev"
		}
	}
}